<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.azure.app.mapper.ArticleMapper">
	<resultMap type="Article" id="ArticleResultMap">
		<id column="id" property="id" />
		<result column="articleTitle" property="articleTitle" />
		<result column="articleAuthor" property="articleAuthor" />
		<result column="articleContent" property="articleContent" />
		<result column="addTime" property="addTime" />
		<result column="updateTime" property="updateTime" />
		<result column="isPublish" property="isPublish" />
		<result column="viewCount" property="viewCount" />
	</resultMap>

</mapper>